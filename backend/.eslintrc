{
  "env": {
    "node": true,
    "es2021": true,
    "jest":true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": ["error", { "allow": ["error"] }]
  }
}
